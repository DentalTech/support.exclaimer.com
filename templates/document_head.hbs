<!--
Script to check if department Technical Sales is online
if dep = Online - button will show
if dep = Offline - button will hide
-->
<script>
zE(function(){
  $zopim(function() {
    $zopim.livechat.setOnConnected(function() {
      var dep = $zopim.livechat.departments.getDepartment('Technical Support');
      {
        if(dep.status == "offline"){
          $zopim.livechat.button.hide();
        }
        else{
          $zopim.livechat.button.show();
        }
      }
    });
  });
});
</script>

